@article{cochran_tryonia_2014,
	title = {Tryonia, a new taenitidoid fern genus segregated from {Jamesonia} and {Eriosorus} ({Pteridaceae})},
	issn = {1314-2011},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4023336},
